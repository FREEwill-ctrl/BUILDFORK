<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="0.0dip"
    android:layout_marginBottom="0.0dip">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0.0dip"
        android:layout_marginBottom="0.0dip"
        android:layout_marginStart="0.0dip"
        android:layout_marginEnd="0.0dip">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.0">
            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                android:onClick="uwuRepository"
                android:layout_marginEnd="8.0dip"
                app:cardBackgroundColor="@color/uwu_bg_card"
                app:cardCornerRadius="24.0dip"
                app:cardElevation="0.0dip"
                app:strokeWidth="0.0dip">
                <FrameLayout
                    android:background="#00000000"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_gravity="start|center|top"
                        android:orientation="horizontal"
                        android:paddingTop="18.0dip"
                        android:paddingBottom="16.0dip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:paddingStart="16.0dip"
                        android:paddingEnd="16.0dip">
                        <TextView
                            android:textSize="18.0sp"
                            android:textColor="?android:textColorPrimary"
                            android:ellipsize="end"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/uwu_github_title"
                            android:singleLine="false" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_gravity="end|center|top"
                        android:orientation="horizontal"
                        android:paddingTop="16.0dip"
                        android:paddingBottom="16.0dip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:paddingStart="16.0dip"
                        android:paddingEnd="16.0dip">
                        <LinearLayout
                            android:background="@drawable/uwu_bg_circle_arrow"
                            android:layout_width="28.0dip"
                            android:layout_height="28.0dip"
                            android:backgroundTint="@color/uwu_bg_dialog_bottomshet">
                            <ImageView
                                android:layout_width="25.0dip"
                                android:layout_height="25.0dip"
                                android:layout_marginTop="1.0dip"
                                android:src="@drawable/uwu_arrow"
                                android:layout_marginStart="2.0dip"
                                app:tint="?android:textColorPrimary" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_width="85.0dip"
                            android:layout_height="85.0dip"
                            android:src="@drawable/uwu_github"
                            app:tint="?attr/colorPrimary" />
                    </LinearLayout>
                    <RelativeLayout
                        android:gravity="bottom|center"
                        android:layout_gravity="bottom|center"
                        android:paddingTop="10.0dip"
                        android:paddingBottom="10.0dip"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1.0"
                        android:layout_marginStart="20.0dip"
                        android:layout_marginEnd="20.0dip">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <TextView
                                android:textSize="14.0sp"
                                android:textColor="?android:textColorSecondary"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:layout_gravity="start|center"
                                android:paddingTop="0.0dip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/uwu_github_summary"
                                android:singleLine="false" />
                        </LinearLayout>
                    </RelativeLayout>
                </FrameLayout>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.0">
            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.0dip"
                android:layout_marginBottom="8.0dip"
                android:onClick="uwuCredits"
                android:layout_marginStart="8.0dip"
                app:cardBackgroundColor="@color/uwu_bg_card"
                app:cardCornerRadius="24.0dip"
                app:cardElevation="0.0dip"
                app:strokeWidth="0.0dip">
                <FrameLayout
                    android:background="#00000000"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_gravity="center"
                        android:orientation="vertical"
                        android:paddingTop="18.0dip"
                        android:paddingBottom="16.0dip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:paddingStart="16.0dip"
                        android:paddingEnd="16.0dip">
                        <ImageView
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:id="@+id/icon_uwu_credits"
                            android:layout_width="25.0dip"
                            android:layout_height="25.0dip"
                            android:src="@drawable/uwu_credits"
                            app:tint="?attr/colorPrimary" />
                        <TextView
                            android:textSize="14.0sp"
                            android:textColor="?android:textColorSecondary"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8.0dip"
                            android:text="@string/uwu_credit_title"
                            android:singleLine="true"
                            android:layout_below="@+id/icon_uwu_credits" />
                    </LinearLayout>
                </FrameLayout>
            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8.0dip"
                android:layout_marginBottom="0.0dip"
                android:onClick="uwuUpdater"
                android:layout_marginStart="8.0dip"
                app:cardBackgroundColor="@color/uwu_bg_card"
                app:cardCornerRadius="24.0dip"
                app:cardElevation="0.0dip"
                app:strokeWidth="0.0dip">
                <FrameLayout
                    android:background="#00000000"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_gravity="center"
                        android:orientation="vertical"
                        android:paddingTop="18.0dip"
                        android:paddingBottom="16.0dip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:paddingStart="16.0dip"
                        android:paddingEnd="16.0dip">
                        <ImageView
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:id="@+id/icon_uwu_update"
                            android:layout_width="25.0dip"
                            android:layout_height="25.0dip"
                            android:src="@drawable/uwu_icon_update"
                            app:tint="?attr/colorPrimary" />
                        <TextView
                            android:textSize="14.0sp"
                            android:textColor="?android:textColorSecondary"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8.0dip"
                            android:text="@string/uwu_updater_title"
                            android:singleLine="true"
                            android:layout_below="@+id/icon_uwu_update" />
                    </LinearLayout>
                </FrameLayout>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>